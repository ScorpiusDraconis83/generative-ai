{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "output": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**", "obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "_site/**", "obj/**"
        ]
      }
    ],
    "output": "../artifacts/_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appName": "Gemini SDK for .NET",
      "_appTitle": "Gemini SDK for .NET",
      "_appLogoPath": "images/mscc.png",
      "_appFaviconPath": "images/mscc.png",
      "_appFooter": "<span>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>",
      "_enableSearch": true,
      "_disableNextArticle": true,
      "pdf": false
    },
    "sitemap": {
      "baseUrl": "https://dotnet.github.io/docfx",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "markdownEngineProperties": {
      "alerts": {
        "TODO": "alert alert-secondary"
      }
    }
  }
}